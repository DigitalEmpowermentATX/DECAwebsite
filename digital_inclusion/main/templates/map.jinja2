{% extends "base.jinja2" %} 
{% load static %} 
{% block content %}
<div class="container">
    <style>
        #map {
            height: 80vh;
            width: 100%;
        }
    </style>
    <div id="map"></div>
    <script>
        var map, infoWindow;
        function initMap() {
            var atx = { lat: 30.267, lng: -97.743 };
            var map = new google.maps.Map(document.getElementById('map'), {
                zoom: 12,
                center: atx
            });
            var marker = new google.maps.Marker({
                position: atx,
                map: map
            });
        }
    </script>
    <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD4Gkn1pvJSIDhIzZSh_zFqlwbp0NMByA0&callback=initMap">
    </script>



</div>
<!-- /.container -->

{% endblock %}